#/bin/sh
user=$1
echo ""
echo " Command      : Function Description"
echo "-------------------------------------------"
echo " exit         : exit shell"
echo " help         : print this help information"
[ "x${user:0:3}" == "xcht" ] && echo " monitor      : exec monitor program"
echo " password     : change [$1] password"
echo " version      : show version information"
echo ""